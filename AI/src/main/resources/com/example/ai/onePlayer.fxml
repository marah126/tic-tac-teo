<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ai.OnePlayer">
    <scene>
        <Scene>
            <AnchorPane prefHeight="343.0" prefWidth="444.0" style="-fx-background-color: #fdebd3;">
            <children>
               <Label alignment="CENTER" layoutX="86.0" layoutY="14.0" prefHeight="89.0" prefWidth="272.0" style="-fx-font-style: #ef8186;" text="Tic-Tac-Teo" textAlignment="CENTER" textFill="#f36a58">
                  <font>
                     <Font name="Bodoni MT Black" size="44.0" />
                  </font>
               </Label>
               <Label alignment="CENTER" layoutX="64.0" layoutY="133.0" prefHeight="25.0" prefWidth="316.0" text="Choose level to play" textFill="#aa493e">
                  <font>
                     <Font name="Bodoni MT Bold" size="24.0" />
                  </font>
               </Label>
               <Button fx:id="hard" layoutX="33.0" layoutY="199.0" mnemonicParsing="false" onAction="#playLevel" prefHeight="78.0" prefWidth="107.0" style="-fx-background-color: #679186;" text="Hard" textFill="#fdebd3">
                  <font>
                     <Font name="Bodoni MT Bold" size="28.0" />
                  </font>
               </Button>
               <Button fx:id="medium" layoutX="156.0" layoutY="199.0" mnemonicParsing="false" onAction="#playLevel" prefHeight="78.0" prefWidth="132.0" style="-fx-background-color: #679186;" text="Medium" textFill="#fdebd3">
                  <font>
                     <Font name="Bodoni MT Bold" size="26.0" />
                  </font>
               </Button>
               <Button fx:id="easy" layoutX="305.0" layoutY="199.0" mnemonicParsing="false" onAction="#playLevel" prefHeight="78.0" prefWidth="107.0" style="-fx-background-color: #679186;" text="Easy" textFill="#fdebd3">
                  <font>
                     <Font name="Bodoni MT Bold" size="28.0" />
                  </font>
               </Button>
               <Button fx:id="back" layoutX="196.0" layoutY="295.0" mnemonicParsing="false" onAction="#goBack" prefHeight="34.0" prefWidth="52.0" style="-fx-background-color: #aa493e;" text="Back" textFill="#d2e8e2">
                  <font>
                     <Font name="Bodoni MT Bold" size="15.0" />
                  </font>
               </Button>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
